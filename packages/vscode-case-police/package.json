{
  "publisher": "antfu",
  "name": "case-police",
  "displayName": "Case Police",
  "version": "0.1.7",
  "private": true,
  "packageManager": "pnpm@8.1.1",
  "description": "Quickly delete all node_modules with one click",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/case-police#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/case-police"
  },
  "bugs": {
    "url": "https://github.com/antfu/case-police/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "CasePolice",
      "properties": {
        "casePolice.dict": {
          "type": "object",
          "default": {},
          "description": "Custom dictionary, will be merged with original dict"
        },
        "casePolice.noDefault": {
          "type": "boolean",
          "default": false,
          "description": "Disable the default dictionary"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "nr build --watch"
  },
  "dependencies": {
    "tsup": "^6.7.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "vsce": "^2.15.0"
  }
}
