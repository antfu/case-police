    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/ni": "^0.20.0",
