    "picocolors": "^1.0.0",
    "pnpm": "^7.28.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  }
